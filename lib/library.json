{
	"name": "czmut",
	"description": "Catch2 inspired unit test framework for microcontrollers"
	"keywords": "unit tests",
	"homepage": "https://crazygaze.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/ruifig/czmut.git"
	},
	"version": "0.0.1",
	"authors":{
		"name" : "Rui Figueira",
		"url": "https://crazygaze.com"
	},
	"export" :
	{
		"include": [
		],
		"exclude": [
			"CMakeLists.txt",
			"*/CMakeLists.txt"
		]
	},
	"frameworks": "*",
	"platforms": "*",
	"build" : {
		"unflags" : ["-std=c++11", "-std=gnu+11"],
		"flags" : [
			"-std=c++17",
			"-std=gnu++17",
			"-I include"
		],
		"srcDir" : ".",
		"srcFilter" : "+<*> -<examples>"
	}
}

